{
  "manifest_version": 2,
  "name": "lyrics",
  "description": "A browser plugin",
  "version": "0.1.0",
  "browser_action": {
    "default_icon": "icon-19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
 
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["lyrics.js"]
    }
  ]
}
